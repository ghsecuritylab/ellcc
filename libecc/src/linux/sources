VPATH := $(VPATH):$(SRCPATH)/$(OS)

# Startup code.
CRTSRCS = crt0.c crtbegin.c crtend.c

SRCS += brk.c fcntl.c stat.c chmod.c fstat.c lseek.c unlink.c \
        chown.c _exit.c getpid.c lstat.c utime.c close.c fchmod.c \
        open.c utimes.c fchown.c kill.c read.c write.c gettimeofday.c \
        ioctl.c sigaction.c sigprocmask.c rmdir.c getuid.c geteuid.c \
        getgid.c getegid.c getpgrp.c setpgid.c killpg.c chdir.c access.c \
        umask.c getgroups.c setgroups.c setrlimit.c getrlimit.c dup.c \
        readlink.c creat.c sigsuspend.c fork.c pipe.c getrusage.c execve.c \
        nanosleep.c wait4.c setitimer.c getitimer.c getdents.c _getcwd.c \
        mkdir.c link.c symlink.c signame.c fchdir.c lutimes.c lchmod.c \
        lchown.c mknod.c mkfifo.c mmap.c madvise.c munmap.c sync.c fsync.c \
        rename.c vfork.c futimes.c

# Target specific sources.
VPATH := $(VPATH):$(SRCPATH)/$(OS)/$(ARCH)
SRCS += start.S
