# Target specific sources are used first.
VPATH := $(VPATH):$(SRCPATH)/m/$(ARCH)
# i386 and x86_64 only. Currently other targets use the noop implementation in m/src/fenv.c
SRCS += fenv.c

VPATH := $(VPATH):$(SRCPATH)/m/src
SRCS += s_floor.c w_sqrt.c e_sqrt.c w_atan2.c e_atan2.c k_standard.c s_atan.c \
        s_matherr.c s_lib_version.c w_log.c e_log.c w_hypot.c e_hypot.c s_rint.c \
        s_finite.c w_sqrtf.c e_sqrtf.c w_logf.c e_logf.c s_isnanf.c w_atan2f.c e_atan2f.c \
        w_hypotf.c e_hypotf.c s_atanf.c s_finitef.c w_cosh.c e_cosh.c w_sinh.c e_sinh.c \
        w_exp.c e_exp.c s_expm1.c w_expf.c e_expf.c w_coshf.c e_coshf.c w_sinhf.c e_sinhf.c \
        s_expm1f.c s_cos.c k_cos.c s_sin.c k_sin.c s_cosf.c k_cosf.c s_sinf.c k_sinf.c \
        e_rem_pio2.c k_rem_pio2.c e_rem_pio2f.c k_rem_pio2f.c s_scalbn.c s_scalbnf.c \
        s_floorf.c w_pow.c e_pow.c w_powf.c e_powf.c

VPATH := $(VPATH):$(SRCPATH)/m/complex
SRCS += cacos.c casin.c creal.c cimag.c csqrt.c clog.c cabs.c cacosf.c casinf.c \
